<UserControl x:Class="TravelManagement.Views.Contents.DataStatictics"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TravelManagement.Views.Contents"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <mui:ModernTab Layout="List" SelectedSource="/Views/Contents/PedestrianVolumeStatistics.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="景区人流量统计" Source="/Views/Contents/PedestrianVolumeStatistics.xaml" />
                <mui:Link DisplayName="紧急呼救统计" Source="/Views/Contents/EmergencyCallStatistics.xaml" />
                <mui:Link DisplayName="异常情况统计" Source="/Views/Contents/LostContactStatistics.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
