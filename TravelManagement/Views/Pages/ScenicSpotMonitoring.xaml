<UserControl x:Class="TravelManagement.Views.Pages.ScenicSpotMonitoring"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TravelManagement.Views.Pages"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <mui:ModernTab Layout="List" SelectedSource="/Views/Contents/TouristManage.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="游客位置显示" Source="/Views/Contents/TouristManage.xaml"/>
                <mui:Link DisplayName="用户查询" Source="/Views/Contents/QueryTourist.xaml"/>
                <mui:Link DisplayName="景区人流量统计" Source="/Views/Contents/PedestrianVolumeStatistics.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
